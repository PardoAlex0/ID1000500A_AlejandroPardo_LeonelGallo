{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/ID1000500A_driver.c",
                "-I",
                "$HOME/ipdi/wrappers/C/include",
                "${file}",
                "-L",
                "$HOME/ipdi/wrappers/C/lib",                
                "-L",
                "$HOME/ipdi/lib/ip",            
                "-lmw",
                "-laip",
                "-lcaip",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}